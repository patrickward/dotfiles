#compdef run

local -a subcmds
subcmds=( \
  'backup:Run the backup script' \
  'report:Run the client git commit report' \
  'flushdns:Flush the system DNS' \
  'mount:Run the mount script' \
  'ssl:Run the ssl cert checker' \
  'cheat:Show a cheat sheet' \
  'help:Show the help screen' \
)
_describe 'run' subcmds

return
